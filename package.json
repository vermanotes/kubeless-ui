{
  "name": "kubeless-ui",
  "version": "0.1.0",
  "description": "Graphical User Interface for Kubeless",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node bin/compile",
    "lint": "eslint bin build config server src tests",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "jest",
    "build": "better-npm-run build",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-flow-react-proptypes": "^2.1.3",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "base-64": "^0.1.0",
    "better-npm-run": "^0.0.15",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "css-loader": "^0.28.0",
    "cssnano": "^3.7.4",
    "debug": "^2.6.4",
    "enzyme": "^2.8.2",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "font-awesome": "^4.7.0",
    "fs-extra": "^2.1.2",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.7.0",
    "jest": "^19.0.2",
    "jest-css-modules": "^1.1.0",
    "jest-static-stubs": "^0.0.1",
    "json-loader": "^0.5.4",
    "keycode": "^2.1.9",
    "lodash": "^4.17.21",
    "material-ui": "^0.17.4",
    "node-fetch": "^1.6.3",
    "node-sass": "^4.0.0",
    "normalize.css": "^6.0.0",
    "postcss-loader": "^1.1.0",
    "qs": "^6.4.0",
    "react": "^15.5.4",
    "react-ace": "^4.2.1",
    "react-addons-test-utils": "^15.0.0",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-tagsinput": "^3.17.0",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.3",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.6",
    "webpack": "^2.4.1",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
    "codecov": "^2.1.0",
    "flow-bin": "^0.44.2",
    "nodemon": "^1.10.2",
    "redbox-react": "^1.2.10",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "jest": {
    "setupFiles": [
      "./tests/jestsetup.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      ".(jpg|jpeg|gif|png|mp4|mkv|avi|webm|swf|wav|mid)$": "jest-static-stubs/$1",
      ".(css|scss)$": "jest-css-modules"
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  }
}
